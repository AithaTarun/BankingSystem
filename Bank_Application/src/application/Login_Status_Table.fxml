<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Login_Status_Table_Controller">
   <children>
      <TableView fx:id="table_Login" layoutX="14.0" layoutY="14.0" prefHeight="1000.0" prefWidth="1920.0">
        <columns>
          <TableColumn fx:id="table_Login_ID" minWidth="70.0" prefWidth="150.0" text="Account ID" />
          <TableColumn fx:id="table_Login_Fname" prefWidth="200.0" text="First name" />
            <TableColumn fx:id="table_Login_Lname" prefWidth="200.0" text="Last name" />
            <TableColumn fx:id="table_Login_Phone" prefWidth="200.0" text="Phone" />
            <TableColumn fx:id="table_Login_DOB" prefWidth="200.0" text="Date of birth" />
            <TableColumn fx:id="table_Login_Mail" prefWidth="200.0" text="Mail address" />
            <TableColumn fx:id="table_Login_Address" prefWidth="200.0" text="Address" />
            <TableColumn fx:id="table_Login_Status" prefWidth="200.0" text="Status" />
            <TableColumn fx:id="table_Login_Message" prefWidth="200.0" text="Message" />
        </columns>
      </TableView>
      <Button fx:id="btn_Login_Status_Back" layoutX="14.0" layoutY="1014.0" mnemonicParsing="false" onAction="#Login_Status_Back" text="Back">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btn_Login_Status_Proceed" layoutX="1837.0" layoutY="1014.0" mnemonicParsing="false" onAction="#Login_Status_Proceed" text="Proceed">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="16.0" />
         </font>
      </Button>
      <Label fx:id="label_Login_Proceed_Message" layoutX="857.0" layoutY="1020.0" text="Action performed successfully" textFill="#00ff33">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
