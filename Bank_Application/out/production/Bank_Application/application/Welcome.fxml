<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Welcome_Controller">
   <children>
      <MediaView fx:id="Video" fitHeight="1080.0" fitWidth="1920.0" layoutX="1.0" layoutY="6.0" />
      <Button fx:id="btn_Create_Account" layoutX="99.0" layoutY="421.0" mnemonicParsing="false" onAction="#Create_Account" text="Create account">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btn_Login" layoutX="853.0" layoutY="421.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btn_Welcome_Exit" layoutX="514.0" layoutY="508.0" mnemonicParsing="false" onAction="#Exit" text="Exit">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="16.0" />
         </font>
      </Button>
      <ImageView fitHeight="161.0" fitWidth="613.0" layoutX="235.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@welcome.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="68.0" layoutY="192.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="94.0" layoutY="267.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Create_Account.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="809.0" layoutY="267.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Login.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="466.0" layoutY="358.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Exit.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
